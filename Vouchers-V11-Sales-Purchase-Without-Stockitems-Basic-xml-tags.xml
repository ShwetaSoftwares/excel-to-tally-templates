  <!-- VOUCHER-->
  <VOUCHER12>
    <GUID FORMULA="=+&quot;udi-DG4W8R-&quot; &amp; A# &amp; &quot;-&quot; &amp; C# &amp; &quot;-&quot; &amp; E#"/>
    <DATE COLUMNREFERENCE="E"/>
    <EFFECTIVEDATE COLUMNREFERENCE="E"/>
    <VOUCHERTYPENAME COLUMNREFERENCE="C"/>
    <ISINVOICE COLUMNREFERENCE="AE"/>
    <PERSISTEDVIEW FORMULA="=if(trim(AE#[1])=&quot;Yes&quot;,&quot;Invoice Voucher View&quot;,&quot;Accounting Voucher View&quot;)"/>
    <!-- GSTIN / UIN -->
    <PARTYGSTIN FORMULA="=if(len(trim(V#))&gt;0,V#,M#)"/>
    <!-- 2017-09-14: STATENAME is converted to proper-case  -->
    <STATENAME FORMULA="=proper(if(len(trim(O#))=0,trim(J#),&quot;&quot;))"/>
    <STATENAME FORMULA="=proper(trim(S#))"/>

    <COUNTRYNAME FORMULA="=if(len(trim(T#))&gt;0,T#,if(len(trim(K#))&gt;0,K#,&quot;India&quot;))"/>
    <COUNTRYOFRESIDENCE FORMULA="=if(len(trim(T#))&gt;0,T#,if(len(trim(K#))&gt;0,K#,&quot;India&quot;))"/>

    <!-- Place of Supply Last one to be considered-->
    <PLACEOFSUPPLY FORMULA="=proper(trim(if(len(trim(N#))&gt;0,proper(trim(N#)),if(len(trim(S#))&gt;0,S#,J#))))"/>
    <GSTREGISTRATIONTYPE FORMULA="=if(len(trim(U#))&gt;0,U#,if(len(trim(L#))&gt;0,L#,if(len(trim(M#))&gt;0,&quot;Regular&quot;,&quot;Unregistered&quot;)))"/>

    <PARTYLEDGERNAME COLUMNREFERENCE="H"/>
    <VOUCHERNUMBER COLUMNREFERENCE="D"/>
    <REFERENCE COLUMNREFERENCE="F"/>
    <UDF:REFERENCEDATE.LIST DESC="`ReferenceDate`" ISLIST="YES" TYPE="Date">
      <UDF:REFERENCEDATE DESC="`ReferenceDate`" COLUMNREFERENCE="G"/>
    </UDF:REFERENCEDATE.LIST>
    <REFERENCEDATE COLUMNREFERENCE="G"/>
    <NARRATION COLUMNREFERENCE="AB"/>
    <COSTCENTRENAME COLUMNREFERENCE="AA"/>
    <ISCOSTCENTRE FORMULA="=if(len(AA#)=0,&quot;No&quot;,&quot;Yes&quot;)"/>
    <!-- Supplier details for Purchase  -->
    <PARTYNAME FORMULA="=if(len(trim(O#))=0,H#,O#)"/>
    <ADDRESS.LIST SKIP="=And(len(trim(P#))=0,len(trim(Q#))=0,len(trim(R#))=0,len(trim(S#))=0)">
      <ADDRESS COLUMNREFERENCE="P"/>
      <ADDRESS COLUMNREFERENCE="Q"/>
      <ADDRESS COLUMNREFERENCE="R"/>
      <ADDRESS COLUMNREFERENCE="S"/>
    </ADDRESS.LIST>
    <!-- Buyer details for Sales -->
    <BASICBUYERNAME FORMULA="=if(len(trim(O#))=0,H#,O#)"/>
    <BASICBUYERADDRESS.LIST SKIP="=And(len(trim(P#))=0,len(trim(Q#))=0,len(trim(R#))=0,len(trim(S#))=0)">
      <BASICBUYERADDRESS COLUMNREFERENCE="P"/>
      <BASICBUYERADDRESS COLUMNREFERENCE="Q"/>
      <BASICBUYERADDRESS COLUMNREFERENCE="R"/>
    </BASICBUYERADDRESS.LIST>

    <!-- Other GST details -->
    <GSTNATUREOFRETURN SKIP="=IsError(Match(trim(B#[1]),\123&quot;credit note&quot;,&quot;debit note&quot;\125,0))" FORMULA="=if(len(trim(W#))=0,&quot;01-Sales Return&quot;,W#)"/>
    <VATPARTYTRANSRETURNNUMBER SKIP="=IsError(Match(trim(B#[1]),\123&quot;credit note&quot;,&quot;debit note&quot;\125,0))" FORMULA="=if(len(trim(X#))=0,F#,X#)"/>
    <VATPARTYTRANSRETURNDATE SKIP="=IsError(Match(trim(B#[1]),\123&quot;credit note&quot;,&quot;debit note&quot;\125,0))" FORMULA="=if(len(trim(Y#))=0,G#,Y#)"/>
    <URDORIGINALSALEVALUE SKIP="=IsError(Match(trim(B#[1]),\123&quot;credit note&quot;,&quot;debit note&quot;\125,0))" FORMULA="=if(len(trim(Z#))=0,&quot;Lesser than or equal to 2.5 lakhs&quot;,Z#)"/>
    <!-- Sales, Purchase Return   : Party Ledger to be Debited -->
    <!-- Purchase, Sales Return   : Party Ledger to be Credited -->
    <ALLLEDGERENTRIES.LIST ALTERNATETAGNAME="=if(trim(AE#[1])=&quot;yes&quot;,&quot;LEDGERENTRIES.LIST&quot;,&quot;&quot;)">
      <LEDGERNAME COLUMNREFERENCE="H"/>
      <ISDEEMEDPOSITIVE FORMULA="=if(Or(lower(trim(B#))=&quot;purchase&quot;,lower(trim(B#))=&quot;credit note&quot;),&quot;No&quot;,&quot;Yes&quot;)"/>
      <AMOUNT FORMULA="=Round({Round(AG#,2)+Round(AI#,2)+Round(AK#,2)+Round(AM#,2)+Round(AO#,2)}+{Round(AQ#,2)+Round(AS#,2)+Round(AU#,2)+Round(AW#,2)+Round(AY#,2)}+{Round(BA#,2)+Round(BC#,2)+Round(BE#,2)+Round(BG#,2)+Round(BI#,2)}+{Round(BK#,2)+Round(BM#,2)+Round(BO#,2)+Round(BQ#,2)+Round(BS#,2)}+{Round(BU#,2)+Round(BW#,2)+Round(BY#,2)+Round(CA#,2)+Round(CC#,2)}+{Round(CE#,2)+Round(CG#,2)+Round(CI#,2)+Round(CK#,2)+Round(CM#,2)},if(trim(AC#)=&quot;yes&quot;,0,2))*if(Or(lower(trim(B#))=&quot;purchase&quot;,lower(trim(B#))=&quot;credit note&quot;),1,-1)"/>
      <BILLALLOCATIONS.LIST SKIP="=Len(Trim(F#))=0">
        <NAME COLUMNREFERENCE="F"/>
        <BILLTYPE>New Ref</BILLTYPE>
        <AMOUNT FORMULA="=Round({Round(AG#,2)+Round(AI#,2)+Round(AK#,2)+Round(AM#,2)+Round(AO#,2)}+{Round(AQ#,2)+Round(AS#,2)+Round(AU#,2)+Round(AW#,2)+Round(AY#,2)}+{Round(BA#,2)+Round(BC#,2)+Round(BE#,2)+Round(BG#,2)+Round(BI#,2)}+{Round(BK#,2)+Round(BM#,2)+Round(BO#,2)+Round(BQ#,2)+Round(BS#,2)}+{Round(BU#,2)+Round(BW#,2)+Round(BY#,2)+Round(CA#,2)+Round(CC#,2)}+{Round(CE#,2)+Round(CG#,2)+Round(CI#,2)+Round(CK#,2)+Round(CM#,2)},if(trim(AC#)=&quot;yes&quot;,0,2))*if(Or(lower(trim(B#))=&quot;purchase&quot;,lower(trim(B#))=&quot;credit note&quot;),1,-1)"/>
      </BILLALLOCATIONS.LIST>
    </ALLLEDGERENTRIES.LIST>
    <!-- Sales/Purchase and Tax Ledgers  -->
    <!-- LEDGER 1 -->
    <ALLLEDGERENTRIES.LIST GROUPBY="=trim(AF#)" ALTERNATETAGNAME="=if(trim(AE#[1])=&quot;yes&quot;,&quot;LEDGERENTRIES.LIST&quot;,&quot;&quot;)" SKIP="={Round(AG#,2)}=0">
      <LEDGERNAME COLUMNREFERENCE="AF"/>
      <!-- For Invoice Mode -->
      <ISDEEMEDPOSITIVE SKIP="=Not(And(Len(trim(AE#[1]))&gt;0,trim(AE#[1])=&quot;Yes&quot;))" FORMULA="=if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;credit note&quot;\125,0))),&quot;Yes&quot;,&quot;No&quot;)"/>
      <!--                
        For Voucher mode  
        ISDEEMEDPOSITIVE
        purchase,credit note and AG# greater than 0   Yes
        sales,debit note and AG# less than 0          Yes
        Otherwise                     No
      -->
      <ISDEEMEDPOSITIVE SKIP="=And(Len(trim(AE#[1]))&gt;0,trim(AE#[1])=&quot;Yes&quot;)" FORMULA="=if(Or(And(Or(trim($B#[1])=&quot;purchase&quot;,trim($B#[1])=&quot;credit note&quot;),{Round(AG#,2)}&gt;0),And(Or(trim($B#[1])=&quot;sales&quot;,trim($B#[1])=&quot;debit note&quot;),{Round(AG#,2)}&lt;0)),&quot;Yes&quot;,&quot;No&quot;)"/>
      <AMOUNT FORMULA="={Round(AG#,2)}*if(Or(trim($B#[1])=&quot;purchase&quot;,trim($B#[1])=&quot;credit note&quot;),-1,1)"/>
    </ALLLEDGERENTRIES.LIST>
    <!-- LEDGER 2 -->
    <ALLLEDGERENTRIES.LIST GROUPBY="=trim(AH#)" ALTERNATETAGNAME="=if(trim(AE#[1])=&quot;yes&quot;,&quot;LEDGERENTRIES.LIST&quot;,&quot;&quot;)" SKIP="={Round(AI#,2)}=0">
      <LEDGERNAME COLUMNREFERENCE="AH"/>
      <!-- For Invoice Mode -->
      <ISDEEMEDPOSITIVE SKIP="=Not(And(Len(trim(AE#[1]))&gt;0,trim(AE#[1])=&quot;Yes&quot;))" FORMULA="=if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;credit note&quot;\125,0))),&quot;Yes&quot;,&quot;No&quot;)"/>
      <!-- For Voucher Mode -->
      <ISDEEMEDPOSITIVE SKIP="=And(Len(trim(AE#[1]))&gt;0,trim(AE#[1])=&quot;Yes&quot;)" FORMULA="=if(Or(And(Or(trim($B#[1])=&quot;purchase&quot;,trim($B#[1])=&quot;credit note&quot;),{Round(AI#,2)}&gt;0),And(Or(trim($B#[1])=&quot;sales&quot;,trim($B#[1])=&quot;debit note&quot;),{Round(AI#,2)}&lt;0)),&quot;Yes&quot;,&quot;No&quot;)"/>
      <AMOUNT FORMULA="={Round(AI#,2)}*if(Or(trim($B#[1])=&quot;purchase&quot;,trim($B#[1])=&quot;credit note&quot;),-1,1)"/>
    </ALLLEDGERENTRIES.LIST>
    <!-- LEDGER 3 -->
    <ALLLEDGERENTRIES.LIST GROUPBY="=trim(AJ#)" ALTERNATETAGNAME="=if(trim(AE#[1])=&quot;yes&quot;,&quot;LEDGERENTRIES.LIST&quot;,&quot;&quot;)" SKIP="={Round(AK#,2)}=0">
      <LEDGERNAME COLUMNREFERENCE="AJ"/>
      <!-- For Invoice Mode -->
      <ISDEEMEDPOSITIVE SKIP="=Not(And(Len(trim(AE#[1]))&gt;0,trim(AE#[1])=&quot;Yes&quot;))" FORMULA="=if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;credit note&quot;\125,0))),&quot;Yes&quot;,&quot;No&quot;)"/>
      <!-- For Voucher Mode -->
      <ISDEEMEDPOSITIVE SKIP="=And(Len(trim(AE#[1]))&gt;0,trim(AE#[1])=&quot;Yes&quot;)" FORMULA="=if(Or(And(Or(trim($B#[1])=&quot;purchase&quot;,trim($B#[1])=&quot;credit note&quot;),{Round(AK#,2)}&gt;0),And(Or(trim($B#[1])=&quot;sales&quot;,trim($B#[1])=&quot;debit note&quot;),{Round(AK#,2)}&lt;0)),&quot;Yes&quot;,&quot;No&quot;)"/>
      <AMOUNT FORMULA="={Round(AK#,2)}*if(Or(trim($B#[1])=&quot;purchase&quot;,trim($B#[1])=&quot;credit note&quot;),-1,1)"/>
    </ALLLEDGERENTRIES.LIST>
    <!-- LEDGER 4 -->
    <ALLLEDGERENTRIES.LIST GROUPBY="=trim(AL#)" ALTERNATETAGNAME="=if(trim(AE#[1])=&quot;yes&quot;,&quot;LEDGERENTRIES.LIST&quot;,&quot;&quot;)" SKIP="={Round(AM#,2)}=0">
      <LEDGERNAME COLUMNREFERENCE="AL"/>
      <!-- For Invoice Mode -->
      <ISDEEMEDPOSITIVE SKIP="=Not(And(Len(trim(AE#[1]))&gt;0,trim(AE#[1])=&quot;Yes&quot;))" FORMULA="=if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;credit note&quot;\125,0))),&quot;Yes&quot;,&quot;No&quot;)"/>
