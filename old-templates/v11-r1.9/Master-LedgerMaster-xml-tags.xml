<!-- These XML tags are added to Excel sheet at run-time by UDIMAGIC  -->
<XMLTAGS CELLREFERENCE="A1" FROMROW="5" xmlns:UDF="TallyUDF">
  <!-- Specifies that this (i.e Column as mentioned above) is the key field -->
  <COLUMNNAME.LIST>
    <COLUMNNAME>NAME</COLUMNNAME>
    <COLUMNNAME>ID</COLUMNNAME>
  </COLUMNNAME.LIST>
  <MASTER TYPE="GROUP">
    <NAME.LIST>
      <NAME COLUMNREFERENCE="E"/>
    </NAME.LIST>
    <PARENT COLUMNREFERENCE="F"/>
  </MASTER>
  <MASTER TYPE="CURRENCY">
    <ORIGINALNAME COLUMNREFERENCE="K"/>
    <EXPANDEDSYMBOL FORMULA="=K# &amp; &quot; Curr.&quot;"/>
    <MAILINGNAME FORMULA="=K# &amp; &quot; Curr.&quot;"/>
    <ASORIGINAL>Yes</ASORIGINAL>
    <HASSPACE>No</HASSPACE>
    <INMILLIONS>No</INMILLIONS>
    <DECIMALPLACES> 2</DECIMALPLACES>
  </MASTER>
  <MASTER TYPE="LEDGER">
    <NAME.LIST SKIP="=len(trim(D#))=0">
      <NAME COLUMNREFERENCE="B"/>
      <NAME SCROLL="YES" COLUMNREFERENCE="D"/>
    </NAME.LIST>
    <NAME SKIP="=Not(len(trim(D#))=0)" COLUMNREFERENCE="B"/>
    <OLDNAME COLUMNREFERENCE="C"/>
    <MTIDLEDGER/>
    <ADDITIONALNAME COLUMNREFERENCE="AC"/>
    <PARENT COLUMNREFERENCE="E"/>
    <DESCRIPTION COLUMNREFERENCE="G"/>
    <NARRATION COLUMNREFERENCE="H"/>
    <CURRENCYNAME COLUMNREFERENCE="K"/>
    <CREDITLIMIT COLUMNREFERENCE="P"/>
    <ISCREDITDAYSCHKON COLUMNREFERENCE="Q"/>
    <OVERRIDECREDITLIMIT COLUMNREFERENCE="R"/>
    <OPENINGBALANCE SKIP="=And(len(trim(L#))=0,len(trim(M#))=0)" FORMULA="=Round(L#+M#,2)*if(L#&gt;0,-1,1)"/>
    <ISBILLWISEON COLUMNREFERENCE="N"/>
    <BILLCREDITPERIOD COLUMNREFERENCE="O"/>
    <BILLALLOCATIONS.LIST SKIP="=len(trim(S#))=0" SCROLL="YES">
      <NAME COLUMNREFERENCE="S"/>
      <BILLDATE COLUMNREFERENCE="T"/>
      <BILLCREDITPERIOD COLUMNREFERENCE="U"/>
      <OPENINGBALANCE FORMULA="=Round(V#+W#,2)*if(V#&gt;0,-1,1)"/>
    </BILLALLOCATIONS.LIST>
    <ADDRESS.LIST SKIP="=And(len(trim(AG#))=0,len(trim(AH#))=0,len(trim(AI#))=0,len(trim(AJ#))=0)">
      <ADDRESS COLUMNREFERENCE="AG"/>
      <ADDRESS COLUMNREFERENCE="AH"/>
      <ADDRESS COLUMNREFERENCE="AI"/>
      <ADDRESS COLUMNREFERENCE="AJ"/>
    </ADDRESS.LIST>
    <LEDGERCONTACT COLUMNREFERENCE="X"/>
    <LEDGERPHONE COLUMNREFERENCE="Y"/>
    <LEDGERMOBILE COLUMNREFERENCE="Z"/>
    <LEDGERFAX COLUMNREFERENCE="AA"/>
    <EMAIL COLUMNREFERENCE="AB"/>
    <COUNTRYNAME COLUMNREFERENCE="AD"/>
    <STATENAME FORMULA="=proper(trim(AE#))"/>
    <!--<LEDSTATENAME COLUMNREFERENCE="AE"/>-->
    <PINCODE COLUMNREFERENCE="AF"/>
    <!-- GST fields for PARTY LEDGER -->
    <GSTREGISTRATIONTYPE COLUMNREFERENCE="AK"/>
    <!-- GSTIN / UIN -->
    <PARTYGSTIN COLUMNREFERENCE="AL"/>
    <!-- Party Type -->
    <GSTNATUREOFSUPPLY COLUMNREFERENCE="AM"/>
    <ISECOMMOPERATOR COLUMNREFERENCE="AN"/>
    <ISOTHTERRITORYASSESSEE COLUMNREFERENCE="AO"/>
    <CONSIDERPURCHASEFOREXPORT COLUMNREFERENCE="AP"/>
    <ISTRANSPORTER COLUMNREFERENCE="AQ"/>
    <!-- GST fields for SALES LEDGER -->
    <!-- Is GST Applicable -->
    <GSTAPPLICABLE SKIP="=len(trim(AR#))=0" FORMULA="= &quot;~[~ &quot; &amp; AR#"/>
    <!-- Type of supply -->
    <GSTTYPEOFSUPPLY COLUMNREFERENCE="BA"/>
    <!-- Empty GSTDETAILS tag not required as it will overwrite existing data, if any  -->
    <GSTDETAILS.LIST SKIP="=And(len(trim(AS#))=0,len(trim(AT#))=0,len(trim(AU#))=0,len(trim(AV#))=0,len(trim(AV#))=0,len(trim(AX#))=0)">
      <!-- Applicable date -->
      <APPLICABLEFROM COLUMNREFERENCE="AS"/>
      <!-- HSN/SAC code -->
      <HSNCODE COLUMNREFERENCE="AT"/>
      <!-- HSN description -->
      <HSN COLUMNREFERENCE="AU"/>
      <!-- Taxability -->
      <TAXABILITY COLUMNREFERENCE="AW"/>
      <!-- Nature of Transaction -->
      <GSTNATUREOFTRANSACTION COLUMNREFERENCE="AV"/>
      <ISREVERSECHARGEAPPLICABLE>No</ISREVERSECHARGEAPPLICABLE>
      <ISNONGSTGOODS>No</ISNONGSTGOODS>
      <GSTINELIGIBLEITC>No</GSTINELIGIBLEITC>
      <STATEWISEDETAILS.LIST>
        <STATENAME>~[~ Any</STATENAME>
        <RATEDETAILS.LIST>
          <GSTRATEDUTYHEAD>Central Tax</GSTRATEDUTYHEAD>
          <GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
          <GSTRATE FORMULA="=Round(AX#/2,2)"/>
        </RATEDETAILS.LIST>
        <RATEDETAILS.LIST>
          <GSTRATEDUTYHEAD>State Tax</GSTRATEDUTYHEAD>
          <GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
          <GSTRATE FORMULA="=Round(AX#/2,2)"/>
        </RATEDETAILS.LIST>
        <RATEDETAILS.LIST>
          <GSTRATEDUTYHEAD>Integrated Tax</GSTRATEDUTYHEAD>
          <GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
          <GSTRATE FORMULA="=Round(AX#,2)"/>
        </RATEDETAILS.LIST>
        <RATEDETAILS.LIST>
          <GSTRATEDUTYHEAD>Cess</GSTRATEDUTYHEAD>
          <GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
          <GSTRATE FORMULA="=Round(AY#,2)"/>
        </RATEDETAILS.LIST>
        <RATEDETAILS.LIST>
          <GSTRATEDUTYHEAD>State Cess</GSTRATEDUTYHEAD>
          <GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
          <GSTRATE FORMULA="=Round(AZ#,2)"/>
        </RATEDETAILS.LIST>
        <GSTSLABRATES.LIST>
        </GSTSLABRATES.LIST>
      </STATEWISEDETAILS.LIST>
      <TEMPGSTDETAILSLABRATES.LIST>
      </TEMPGSTDETAILSLABRATES.LIST>
    </GSTDETAILS.LIST>
    <!-- GST fields for DUTIES AND TAXES LEDGER -->
    <!-- This tag appears for CGST only -->
    <GSTAPPLICABLE SKIP="=Or(len(trim(BC#))=0,Not(trim(BC#)=&quot;central tax&quot;))">~[~ Applicable</GSTAPPLICABLE>
    <!-- GST -->
    <TAXTYPE COLUMNREFERENCE="BB"/>
    <!-- Central Tax or State Tax or Integrated Tax -->
    <GSTDUTYHEAD COLUMNREFERENCE="BC"/>
    <!-- GST Tax Rate -->
    <RATEOFTAXCALCULATION COLUMNREFERENCE="BD"/>
    <!-- Always blank -->
    <GSTTYPE/>
    <!-- FOR VAT -->
    <TAXTYPE COLUMNREFERENCE="BE"/>
    <!-- If VAT Rate is specified and is greater than 0, then set the tag VATAPPLICABLE -->
    <VATAPPLICABLE SKIP="=Round(BG#,2)=0">~[~ Applicable</VATAPPLICABLE>
    <RATEOFTAXCALCULATION COLUMNREFERENCE="BG"/>
    <VATDETAILS.LIST SKIP="=len(trim(BF#))=0">
      <FROMDATE COLUMNREFERENCE="BJ"/>
      <TAXTYPE COLUMNREFERENCE="BI"/>
      <STATNATURENAME COLUMNREFERENCE="BF"/>
      <ISINVDETAILSENABLE>Yes</ISINVDETAILSENABLE>
      <ISCALCONACTUALQTY>No</ISCALCONACTUALQTY>
      <RATEOFVAT COLUMNREFERENCE="BG"/>
      <ADDITIONALTAX COLUMNREFERENCE="BH"/>
    </VATDETAILS.LIST>
    <INCOMETAXNUMBER COLUMNREFERENCE="BK"/>
    <REGISTRATIONNUMBER COLUMNREFERENCE="BL"/>
    <VATDEALERTYPE COLUMNREFERENCE="BM"/>
    <VATTINNUMBER COLUMNREFERENCE="BN"/>
    <SALESTAXNUMBER COLUMNREFERENCE="BN"/>
    <UDF:VATREGISTRATIONDATE.LIST DESC="`VATRegistrationDate`" ISLIST="YES" TYPE="Date" SKIP="=len(trim(BO#))=0">
      <UDF:VATREGISTRATIONDATE DESC="`VATRegistrationDate`" COLUMNREFERENCE="BO"/>
    </UDF:VATREGISTRATIONDATE.LIST>
    <INTERSTATESTNUMBER COLUMNREFERENCE="BP"/>
    <UDF:CSTREGISTRATIONDATE.LIST DESC="`CSTRegistrationDate`" ISLIST="YES" TYPE="Date" SKIP="=len(trim(BQ#))=0">
      <UDF:CSTREGISTRATIONDATE DESC="`CSTRegistrationDate`" COLUMNREFERENCE="BQ"/>
    </UDF:CSTREGISTRATIONDATE.LIST>
    <ISTDSAPPLICABLE COLUMNREFERENCE="CE"/>
    <ISSTXPARTY COLUMNREFERENCE="CF"/>
    <PAYMENTDETAILS.LIST SKIP="=And(len(trim(CA#))=0,len(trim(CB#))=0,len(trim(CC#))=0)">
      <IFSCODE COLUMNREFERENCE="CA"/>
      <BANKNAME COLUMNREFERENCE="CC"/>
      <ACCOUNTNUMBER COLUMNREFERENCE="CB"/>
      <PAYMENTFAVOURING COLUMNREFERENCE="BY"/>
      <TRANSACTIONNAME COLUMNREFERENCE="BW"/>
      <SETASDEFAULT COLUMNREFERENCE="BZ"/>
      <DEFAULTTRANSACTIONTYPE FORMULA="=IF(LEN(TRIM(BX#))=0,&quot;Inter Bank Transfer&quot;,BX#)"/>
    </PAYMENTDETAILS.LIST>
    <AFFECTSSTOCK COLUMNREFERENCE="I"/>
    <EXCISEREGNO COLUMNREFERENCE="BR"/>
    <EXCISEREGISTRATIONDATE COLUMNREFERENCE="BS"/>
    <EXCISERANGE COLUMNREFERENCE="BT"/>
    <EXCISEDIVISION COLUMNREFERENCE="BU"/>
    <EXCISECOMMISSIONERATE COLUMNREFERENCE="BV"/>
    <ISCOSTCENTRESON COLUMNREFERENCE="J"/>
    <PRICELEVEL COLUMNREFERENCE="CD"/>
  </MASTER>
</XMLTAGS>

