      <BASICBUYERADDRESS COLUMNREFERENCE="O"/>
      <BASICBUYERADDRESS COLUMNREFERENCE="P"/>
      <BASICBUYERADDRESS COLUMNREFERENCE="Q"/>
    </BASICBUYERADDRESS.LIST>
    <PLACEOFSUPPLY COLUMNREFERENCE="R"/>
    <GSTREGISTRATIONTYPE COLUMNREFERENCE="S"/>
