    <!-- GSTIN / UIN -->
    <PARTYGSTIN FORMULA="=if(len(trim(M#))=0,trim(L#),&quot;&quot;)"/>
    <PARTYGSTIN FORMULA="=trim(T#)"/>
    <!-- 2017-09-14: STATENAME is converted to proper-case  -->
