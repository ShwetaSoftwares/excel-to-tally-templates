<!-- These XML tags are added to Excel sheet Cell A1 at run-time by UDIMAGIC  -->
<XMLTAGS CELLREFERENCE="A1" FROMROW="5" xmlns:UDF="TallyUDF">
  <!-- Specifies that this (i.e Column A as mentioned above) is the key field -->
  <COLUMNNAME.LIST>
    <COLUMNNAME>ID</COLUMNNAME>
  </COLUMNNAME.LIST>
  <!--Create a single COSTCATEGORY Master-->
  <MASTER TYPE="COSTCATEGORY" SINGLERECORD="YES">
    <NAME.LIST>
      <NAME>EMPLOYEE</NAME>
    </NAME.LIST>
    <AFFECTSSTOCK>NO</AFFECTSSTOCK>
    <ALLOCATEREVENUE>YES</ALLOCATEREVENUE>
    <ALLOCATENONREVENUE>YES</ALLOCATENONREVENUE>
  </MASTER>
  <!-- Create Employee group Masters -->
  <MASTER TYPE="COSTCENTRE">
    <NAME.LIST>
      <NAME COLUMNREFERENCE="G"/>
    </NAME.LIST>
    <CATEGORY>EMPLOYEE</CATEGORY>
    <AFFECTSSTOCK>No</AFFECTSSTOCK>
    <FORPAYROLL>Yes</FORPAYROLL>
    <!-- Specifies that this is a GROUP -->
    <ISEMPLOYEEGROUP>Yes</ISEMPLOYEEGROUP>
  </MASTER>
  <!-- Create Employee Masters -->
  <MASTER TYPE="COSTCENTRE">
    <!-- Condition to modify Employee Master -->
    <IMPORTDUPS FORMULA="=if(And(Not(IsError(AB#)),len(trim(AB#))&gt;0,AB#=&quot;yes&quot;),&quot;Modify with new data&quot;,&quot;Ignore Duplicates&quot;)"/>
    <NAME.LIST SKIP="=Or(len(trim(E#))=0,len(trim(F#))=0)">
      <NAME COLUMNREFERENCE="E"/>
      <NAME COLUMNREFERENCE="F"/>
    </NAME.LIST>
    <NAME SKIP="=Not(len(trim(F#))=0)" COLUMNREFERENCE="E"/>
    <OLDNAME FORMULA="=if(len(trim(F#))=0,E#,F#)"/>
    <ADDITIONALNAME.LIST SKIP="=len(trim(I#))=0">
      <ADDITIONALNAME COLUMNREFERENCE="I"/>
    </ADDITIONALNAME.LIST>
    <PARENT COLUMNREFERENCE="G"/>
    <CATEGORY>EMPLOYEE</CATEGORY>
    <DESIGNATION COLUMNREFERENCE="H"/>
    <DATEOFJOIN COLUMNREFERENCE="J"/>
    <DATEOFBIRTH COLUMNREFERENCE="K"/>

    <GENDER COLUMNREFERENCE="L"/>
    <BLOODGROUP COLUMNREFERENCE="M"/>
    <FATHERNAME COLUMNREFERENCE="N"/>
    <SPOUSENAME COLUMNREFERENCE="O"/>
    <ADDRESS.LIST SKIP="=len(trim(P#))=0">
      <ADDRESS COLUMNREFERENCE="P"/>
    </ADDRESS.LIST>

    <CONTACTNUMBERS COLUMNREFERENCE="Q"/>
    <EMAILID COLUMNREFERENCE="R"/>
    <BANKDETAILS COLUMNREFERENCE="S"/>
    <BANKBRANCH COLUMNREFERENCE="T"/>
    <BANKACCOUNTNUMBER COLUMNREFERENCE="U"/>
    <PANNUMBER COLUMNREFERENCE="V"/>
    <AADHARNUMBER COLUMNREFERENCE="W"/>
    <UANNUMBER COLUMNREFERENCE="X"/>
    <PFACCOUNTNUMBER COLUMNREFERENCE="Y"/>
    <PRACCOUNTNUMBER COLUMNREFERENCE="Z"/>
    <ESINUMBER COLUMNREFERENCE="AA"/>

    <AFFECTSSTOCK>No</AFFECTSSTOCK>
    <FORPAYROLL>Yes</FORPAYROLL>
  </MASTER>
  <!-- Create Attendance-type Master-->
  <MASTER TYPE="ATTENDANCETYPE">
    <NAME.LIST>
      <NAME FORMULA="=$AC$2"/>
    </NAME.LIST>
    <PARENT/>
    <BASEUNITS/>
    <ATTENDANCEPRODUCTIONTYPE>Attendance / Leave with Pay</ATTENDANCEPRODUCTIONTYPE>
    <ATTENDANCEPERIOD>Days</ATTENDANCEPERIOD>
  </MASTER>
  <MASTER TYPE="ATTENDANCETYPE">
    <NAME.LIST>
      <NAME FORMULA="=$AD$2"/>
    </NAME.LIST>
    <PARENT/>
    <BASEUNITS/>
    <ATTENDANCEPRODUCTIONTYPE>Attendance / Leave with Pay</ATTENDANCEPRODUCTIONTYPE>
    <ATTENDANCEPERIOD>Days</ATTENDANCEPERIOD>
  </MASTER>
  <MASTER TYPE="ATTENDANCETYPE">
    <NAME.LIST>
      <NAME FORMULA="=$AE$2"/>
    </NAME.LIST>
    <PARENT/>
    <BASEUNITS/>
    <ATTENDANCEPRODUCTIONTYPE>Attendance / Leave with Pay</ATTENDANCEPRODUCTIONTYPE>
    <ATTENDANCEPERIOD>Days</ATTENDANCEPERIOD>
  </MASTER>
  <MASTER TYPE="ATTENDANCETYPE">
    <NAME.LIST>
      <NAME FORMULA="=$AF$2"/>
    </NAME.LIST>
    <PARENT/>
    <BASEUNITS/>
    <ATTENDANCEPRODUCTIONTYPE>Attendance / Leave with Pay</ATTENDANCEPRODUCTIONTYPE>
    <ATTENDANCEPERIOD>Days</ATTENDANCEPERIOD>
  </MASTER>
  <VOUCHER11>
    <!-- Must be unique for each Excel sheet -->
    <GUID FORMULA="=+&quot;udi-H5DU83&quot; &amp; A# &amp; &quot;-&quot; &amp; C# &amp; &quot;-&quot; &amp; A#"/>
    <DATE COLUMNREFERENCE="C"/>
    <EFFECTIVEDATE COLUMNREFERENCE="C"/>
    <!-- VoucherType name is specified directly here -->
    <VOUCHERTYPENAME>ATTENDANCE</VOUCHERTYPENAME>
    <DIFFACTUALQTY>YES</DIFFACTUALQTY>
    <VOUCHERNUMBER COLUMNREFERENCE="A"/>
    <NARRATION FORMULA="=+&quot;Attendance for the month of &quot; &amp; D#"/>
    <!-- Invoice format. YES or NO -->
    <ISINVOICE>No</ISINVOICE>
    <!-- Pick up the value for $AC$2-->
    <ATTENDANCEENTRIES.LIST SCROLL="Yes" SKIP="=Round(AC#,2)=0">
      <NAME FORMULA="=If(len(F#)=0,E#,F#)"/>
      <ATTENDANCETYPE FORMULA="=$AC$2"/>
      <ATTDTYPETIMEVALUE COLUMNREFERENCE="AC"/>
    </ATTENDANCEENTRIES.LIST>
    <!-- Pick up the value for $AD$2-->
    <ATTENDANCEENTRIES.LIST SCROLL="Yes" SKIP="=Round(AD#,2)=0">
      <NAME FORMULA="=If(len(F#)=0,E#,F#)"/>
      <ATTENDANCETYPE FORMULA="=$AD$2"/>
      <ATTDTYPETIMEVALUE COLUMNREFERENCE="AD"/>
    </ATTENDANCEENTRIES.LIST>
    <!-- Pick up the value for $AE$2-->
    <ATTENDANCEENTRIES.LIST SCROLL="Yes" SKIP="=Round(AE#,2)=0">
      <NAME FORMULA="=If(len(F#)=0,E#,F#)"/>
      <ATTENDANCETYPE FORMULA="=$AE$2"/>
      <ATTDTYPETIMEVALUE COLUMNREFERENCE="AE"/>
    </ATTENDANCEENTRIES.LIST>
    <!-- Pick up the value for $AF$2-->
    <ATTENDANCEENTRIES.LIST SCROLL="Yes" SKIP="=Round(AF#,2)=0">
      <NAME FORMULA="=If(len(F#)=0,E#,F#)"/>
      <ATTENDANCETYPE FORMULA="=$AF$2"/>
      <ATTDTYPETIMEVALUE COLUMNREFERENCE="AF"/>
    </ATTENDANCEENTRIES.LIST>
  </VOUCHER11>
</XMLTAGS>