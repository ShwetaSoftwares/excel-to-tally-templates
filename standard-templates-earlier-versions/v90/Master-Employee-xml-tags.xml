<!-- These XML tags are added to Excel sheet Cell A1 at run-time by UDIMAGIC  -->
<XMLTAGS CELLREFERENCE="A1" xmlns:UDF="TallyUDF">
	<!-- Specifies that this (i.e Column A as mentioned above) is the key field -->
	<COLUMNNAME.LIST>
		<COLUMNNAME>ID</COLUMNNAME>
	</COLUMNNAME.LIST>
	<!--Create a single COSTCATEGORY Master-->
	<MASTER TYPE="COSTCATEGORY" SINGLERECORD="YES">
		<NAME.LIST>
			<NAME>EMPLOYEE</NAME>
		</NAME.LIST>
		<AFFECTSSTOCK>NO</AFFECTSSTOCK>
		<ALLOCATEREVENUE>YES</ALLOCATEREVENUE>
		<ALLOCATENONREVENUE>YES</ALLOCATENONREVENUE>
	</MASTER>
	<!-- Create Employee group Masters -->
	<MASTER TYPE="COSTCENTRE">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="D"/>
		</NAME.LIST>
		<CATEGORY>EMPLOYEE</CATEGORY>
		<AFFECTSSTOCK>No</AFFECTSSTOCK>
		<FORPAYROLL>Yes</FORPAYROLL>
		<!-- Specifies that this is a GROUP -->
		<ISEMPLOYEEGROUP>Yes</ISEMPLOYEEGROUP>
	</MASTER>
	<!-- Create Employee Masters -->
	<MASTER TYPE="COSTCENTRE">
		<NAME.LIST SKIP="=Or(len(trim(B#))=0,len(trim(C#))=0)">
			<NAME COLUMNREFERENCE="B"/>
			<NAME COLUMNREFERENCE="C"/>
		</NAME.LIST>
		<NAME SKIP="=Not(len(trim(C#))=0)" COLUMNREFERENCE="B"/>
		<OLDNAME FORMULA="=if(len(trim(C#))=0,B#,C#)"/>
		<ADDITIONALNAME.LIST SKIP="=len(trim(F#))=0">
			<ADDITIONALNAME COLUMNREFERENCE="F"/>
		</ADDITIONALNAME.LIST>
		<PARENT COLUMNREFERENCE="D"/>
		<CATEGORY>EMPLOYEE</CATEGORY>
		<DESIGNATION COLUMNREFERENCE="E"/>
		<DATEOFJOIN COLUMNREFERENCE="G"/>
		<DATEOFBIRTH COLUMNREFERENCE="H"/>
		<GENDER COLUMNREFERENCE="I"/>
		<BLOODGROUP COLUMNREFERENCE="J"/>
		<FATHERNAME COLUMNREFERENCE="K"/>
		<SPOUSENAME COLUMNREFERENCE="L"/>
		<ADDRESS.LIST SKIP="=len(trim(M#))=0">
			<ADDRESS COLUMNREFERENCE="M"/>
		</ADDRESS.LIST>
		<CONTACTNUMBERS COLUMNREFERENCE="N"/>
		<EMAILID COLUMNREFERENCE="O"/>
		<BANKDETAILS COLUMNREFERENCE="P"/>
		<BANKBRANCH COLUMNREFERENCE="Q"/>
		<BANKACCOUNTNUMBER COLUMNREFERENCE="R"/>
		<PANNUMBER COLUMNREFERENCE="S"/>
		<AADHARNUMBER COLUMNREFERENCE="T"/>
		<UANNUMBER COLUMNREFERENCE="U"/>
		<PFACCOUNTNUMBER COLUMNREFERENCE="V"/>
		<PRACCOUNTNUMBER COLUMNREFERENCE="W"/>
		<ESINUMBER COLUMNREFERENCE="X"/>
		<AFFECTSSTOCK>No</AFFECTSSTOCK>
		<FORPAYROLL>Yes</FORPAYROLL>
	</MASTER>
</XMLTAGS>
