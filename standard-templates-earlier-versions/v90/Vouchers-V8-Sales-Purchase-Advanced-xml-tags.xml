<!-- These XML tags are added to Excel sheet at run-time by UDIMAGIC  -->
<!-- 
	\123 is replaced with { at runtime 
	\125 is replaced with } at runtime
	The curly braces are used for Array notation in Excel
-->
<XMLTAGS CELLREFERENCE="A1" xmlns:UDF="TallyUDF">
	<!-- Specifies that this (i.e Column as mentioned above) is the key field -->
	<COLUMNNAME.LIST>
		<COLUMNNAME>ID</COLUMNNAME>
		<COLUMNNAME>NAME</COLUMNNAME>
	</COLUMNNAME.LIST>
	<!-- For macro -->
	<!--VBA LOADFILE="Vouchers-V7-Sales-E-Commerce-macro.txt" RUNMACROATSTARTUP="NewColumns,A#"/-->
	<DATA LOADFILE="Vouchers-V8-Sales-Purchase-Advanced-settings.csv"/>
	<!-- Create VOUCHERTYPE Master -->
	<MASTER TYPE="VOUCHERTYPE">
		<NAME.LIST TYPE="String">
			<NAME COLUMNREFERENCE="C"/>
		</NAME.LIST>
		<PARENT COLUMNREFERENCE="B"/>
		<ISDEEMEDPOSITIVE FORMULA="=if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;credit note&quot;\125,0))),&quot;No&quot;,&quot;Yes&quot;)"/>
		<NUMBERINGMETHOD>Manual</NUMBERINGMETHOD>
	</MASTER>
	<!-- Create Party LEDGER Masters -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME COLUMNREFERENCE="H"/>
		</NAME.LIST>
		<ADDITIONALNAME COLUMNREFERENCE="H"/>
		<PARENT FORMULA="=if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;Sundry Creditors&quot;,&quot;Sundry Debtors&quot;)"/>
		<ISBILLWISEON>Yes</ISBILLWISEON>
		<!-- Required for Tally ERP 9 Release 5.1 and higher -->
		<COUNTRYNAME SKIP="=len(trim(AA#))=0">India</COUNTRYNAME>
    <COUNTRYOFRESIDENCE SKIP="=len(trim(AA#))=0">India</COUNTRYOFRESIDENCE>
		<LEDSTATENAME COLUMNREFERENCE="AA"/>
    <!-- VAT fields -->
		<VATDEALERTYPE SKIP="=len(trim(AA#))=0" COLUMNREFERENCE="K"/>
		<VATTINNUMBER SKIP="=len(trim(AA#))=0" COLUMNREFERENCE="L"/>
    <!-- GST fields -->
    <GSTREGISTRATIONTYPE COLUMNREFERENCE="I"/>
    <PARTYGSTIN COLUMNREFERENCE="J"/>
	</MASTER>
	<!-- Create Sales/Purchase Ledger Master -->
	<MASTER TYPE="LEDGER">
		<NAME.LIST>
			<NAME FORMULA="=&quot;__LOOKUP__(AS# &amp; &quot;-LEDGER-FOR&quot; &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__&quot;"/>
		</NAME.LIST>
		<PARENT FORMULA="=if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;Purchase Accounts&quot;,&quot;Sales Accounts&quot;)"/>
		<TAXTYPE>Others</TAXTYPE>
		<ISBILLWISEON>No</ISBILLWISEON>
		<ISCOSTCENTRESON>No</ISCOSTCENTRESON>
		<AFFECTSSTOCK FORMULA="=if(len(trim(AF#))=0,&quot;No&quot;,&quot;Yes&quot;)"/>

    <!-- IMPORTANT: Same tags given for STOCKITEM -->
    <!-- Possible Values : Applicable, Not Applicable -->
    <GSTAPPLICABLE SKIP="=len(trim(&quot;__LOOKUP__(AS# &amp; &quot;-IS-GST-APPLICABLE&quot;)__&quot;))=0" FORMULA="=&quot;~[~ &quot; &amp; &quot;__LOOKUP__(AS# &amp; &quot;-IS-GST-APPLICABLE&quot;)__&quot;"/>
    <!-- Type of supply : Goods or Services -->
    <GSTTYPEOFSUPPLY FORMULA="=&quot;__LOOKUP__(AS# &amp; &quot;-TYPE-OF-SUPPLY&quot;)__&quot;"/>

    <GSTDETAILS.LIST SKIP="=Not(&quot;applicable&quot;=&quot;__LOOKUP__(AS# &amp; &quot;-IS-GST-APPLICABLE&quot;)__&quot;)">
      <APPLICABLEFROM FORMULA="=&quot;__LOOKUP__(AS# &amp; &quot;-APPLICABLE-FROM-DATE&quot;)__&quot;"/>
      <CALCULATIONTYPE>On Value</CALCULATIONTYPE>
      <HSNMASTERNAME/>
      <HSNCODE COLUMNREFERENCE="AH"/>
      <HSN COLUMNREFERENCE="AI"/>
      <HSNCODE FORMULA="=&quot;__LOOKUP__(AS# &amp; &quot;-HSN-CODE&quot;)__&quot;"/>
      <HSN FORMULA="=&quot;__LOOKUP__(AS# &amp; &quot;-HSN-DESC&quot;)__&quot;"/>
      
      <!-- Possible Values : Nil Rated, Exempt, Taxable  -->
      <TAXABILITY FORMULA="=&quot;__LOOKUP__(AS# &amp; &quot;-TAXABILITY&quot;)__&quot;"/>

      <!-- Nature of Transaction -->
      <GSTNATUREOFTRANSACTION FORMULA="=&quot;__LOOKUP__(AS# &amp; &quot;-GST-NATUREOFTRANSACTION-FOR&quot; &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__&quot;"/>

      <STATEWISEDETAILS.LIST>
        <STATENAME>~[~ Any</STATENAME>
        <RATEDETAILS.LIST>
          <GSTRATEDUTYHEAD>Central Tax</GSTRATEDUTYHEAD>
          <GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
          <GSTRATE FORMULA="=Round(__LOOKUP__(AS# &amp; &quot;-CGST-RATE-FOR&quot; &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__,2)"/>
        </RATEDETAILS.LIST>
        <RATEDETAILS.LIST>
          <GSTRATEDUTYHEAD>State Tax</GSTRATEDUTYHEAD>
          <GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
          <GSTRATE FORMULA="=Round(__LOOKUP__(AS# &amp; &quot;-SGST-RATE-FOR&quot; &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__,2)"/>
        </RATEDETAILS.LIST>
        <RATEDETAILS.LIST>
          <GSTRATEDUTYHEAD>Integrated Tax</GSTRATEDUTYHEAD>
          <GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
          <GSTRATE FORMULA="=&quot;__LOOKUP__(AS# &amp; &quot;-IGST-RATE-FOR&quot; &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__&quot;"/>
        </RATEDETAILS.LIST>
        <RATEDETAILS.LIST>
          <GSTRATEDUTYHEAD>Cess</GSTRATEDUTYHEAD>
          <GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
          <GSTRATE FORMULA="=Round(__LOOKUP__(AS# &amp; &quot;-CESS-RATE-FOR&quot; &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__,2)"/>
        </RATEDETAILS.LIST>
      </STATEWISEDETAILS.LIST>
      <TEMPGSTDETAILSLABRATES.LIST>
      </TEMPGSTDETAILSLABRATES.LIST>
    </GSTDETAILS.LIST>

    <!-- VAT details -->
    <TAXCLASSIFICATIONNAME FORMULA="=&quot;__LOOKUP__(AS# &amp; &quot;-VATCLASS-FOR&quot;  &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__&quot;"/>
		<USEFORVAT>Yes</USEFORVAT>
		<VATAPPLICABLE SKIP="=len(trim(&quot;__LOOKUP__(AS# &amp; &quot;-IS-VAT-APPLICABLE&quot;)__&quot;))=0"
      FORMULA="=&quot;~[~ &quot; &amp; &quot;__LOOKUP__(AS# &amp; &quot;-IS-VAT-APPLICABLE&quot;)__&quot;"/>
		<!-- Required for Tally ERP 9 Release 5.1 and higher -->
		<VATDETAILS.LIST SKIP="=Not(&quot;applicable&quot;=&quot;__LOOKUP__(AS# &amp; &quot;-IS-VAT-APPLICABLE&quot;)__&quot;)">
			<FROMDATE>20100401</FROMDATE>
			<TAXTYPE>Taxable</TAXTYPE>
			<STATNATURENAME FORMULA="=&quot;__LOOKUP__(AS# &amp; &quot;-NATUREOFTRANSACTION-FOR&quot; &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__&quot;"/>
			<ISINVDETAILSENABLE>Yes</ISINVDETAILSENABLE>
			<ISCALCONACTUALQTY>No</ISCALCONACTUALQTY>
			<RATEOFVAT FORMULA="=&quot;__LOOKUP__(AS# &amp; &quot;-VATRATE-FOR&quot;  &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__&quot;"/>
			<ADDITIONALTAX FORMULA="=&quot;__LOOKUP__(AS# &amp; &quot;-ADDLVATRATE-FOR&quot; &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__&quot;"/>
			<VATCOMMODITYNAME FORMULA="=&quot;__LOOKUP__(AS# &amp; &quot;-VATRATE-FOR&quot; &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__&quot;"/>
			<VATCOMMODITYCODE FORMULA="=&quot;__LOOKUP__(AS# &amp; &quot;-VATRATE-FOR&quot; &amp; if(Not(IsError(Match(trim(B#[1]),\123&quot;purchase&quot;,&quot;purchase order&quot;,&quot;receipt note&quot;,&quot;debit note&quot;\125,0))),&quot;PURC&quot;,&quot;SALE&quot;))__&quot;"/>
		</VATDETAILS.LIST>
	</MASTER>

